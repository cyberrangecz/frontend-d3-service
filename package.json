{
  "name": "@muni-kypo-crp/d3-service",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "build-library": "ng build d3-service --configuration production",
    "create-package": "cd dist/d3-service && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd ./projects/d3-service/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/d3-service && npm publish",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "d3": "^7.4.4",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1502.10",
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "~15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.1",
    "typescript": "~4.8.4"
  },
  "optionalDependencies": {
    "esbuild-android-arm64": "^0.15.18",
    "fsevents": "2.1.2"
  }
}
